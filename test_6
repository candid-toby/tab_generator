      var table_all_strings_all_notes = "<table><tr><th>Fret Number</th><th>String 1 Note</th><th>String 2 Note</th><th>String 3 Note</th><th>String 4 Note</th><th>String 5 Note</th><th>String 6 Note</th> </tr>";
      
      for (var i = 1; i <= 21; i++) {
        var noteIndex = (i - 1) % string_1_notes.length;
        var note_1 = string_1_notes[noteIndex];
        var note_2 = string_2_notes[noteIndex];
        var note_3 = string_3_notes[noteIndex];
        var note_4 = string_4_notes[noteIndex];
        var note_5 = string_5_notes[noteIndex];
        var note_6 = string_6_notes[noteIndex];
        table_all_strings_all_notes += "<tr><td>" + i + "</td><td>" + note_1 +"</td><td>" + note_2 +"</td><td>" + note_3 +"</td><td>" + note_4 +"</td><td>" + note_5 +"</td><td>" + note_6 + "</td></tr>";
      }
      table_all_strings_all_notes += "</table>";

      document.getElementById("noteTable").innerHTML = table_all_strings_all_notes;
    }
function generate_feasible_fret_table() {
  var form = document.getElementById("noteForm");
       var string_1_start = form.elements.string_1_start.value;
       var string_2_start = form.elements.string_2_start.value;
       var string_3_start = form.elements.string_3_start.value;
       var string_4_start = form.elements.string_4_start.value;
       var string_5_start = form.elements.string_5_start.value;
       var string_6_start = form.elements.string_6_start.value;
var string_1_notes = [];
if (string_1_start === "a") {
  var string_1_notes = ["A", "A#", "B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#"];
}
else if (string_1_start === "a_sharp") {
  var string_1_notes = ["A#", "B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#", "A" ];
}

else if (string_1_start === "b") {
  var string_1_notes = ["B", "C", "C#", "D", "D#", "E", "F", "F#", "G", "G#", "A", "A#" ];
